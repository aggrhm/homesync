environments:
  ignitio:
    host: ignitio-deploy
    user: centos
    ports:
      - name: Grafana
        local: 4010
        remote: localhost:3000

  ignitio-aws:
    host: ignitio-deploy-aws
    user: ubuntu
    ports:
      - name: Production Kubernetes OKD
        local: 4011
        remote: localhost:9000
      - name: Staging Kubernetes OKD
        local: 8000
        remote: localhost:8000
      - name: Kubernetes Dashboard
        local: 4013
        remote: 192.168.60.143:8443
